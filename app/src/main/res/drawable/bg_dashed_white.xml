<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp">

        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="#B6BABD"
                android:dashGap="6dp"
                android:dashWidth="3dp" />

            <solid android:color="#FFFFFF" />


        </shape>
    </item>
</layer-list>